{
    "name":"irhliving/project",
    "description":"A simple php framework from Bai Ming",
    "keywords": [],
    "license": "MIT",
    "type":"project",
    "autoload":{
        "psr-4":{
            "Simcify\\": "src/",
            "Smolblog\\OAuth2\\Client\\Provider\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "vendor/pecee/simple-router/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Smolblog\\OAuth2\\Client\\Provider\\Test\\": "test/src/"
        }
    },
    "require": {
        "php": ">=5.5",
        "ext-mbstring": "*",
        "vlucas/phpdotenv": "^2.5",
        "phpmailer/phpmailer": "^6.0",
        "gilbitron/easycsrf": "^1.0",
        "php-di/php-di": "^5.4.6",
        "suin/php-image-resizer": ">=1.0.0",
        "gumlet/php-image-resize": "1.9.*",
        "pecee/simple-router": "3.7.0.0",
        "twilio/sdk": "*",
        "africastalking/africastalking": "*",
        "google/apiclient": "^2.12.1",
        "cloudconvert/cloudconvert-php": "2.2.*",
        "oohology/dotenvwriter": "*",
        "paragonie/random_compat": "<9.99",
        "paypal/paypal-checkout-sdk": "1.0.1",
        "phpoffice/phpspreadsheet": "^1.16",
        "facebook/graph-sdk": "^5.1",
        "league/oauth2-client": "^2.6",
        "paragonie/random-lib": "^2.0"
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "test": "phpunit --testdox test/src/",
        "lint": "./vendor/squizlabs/php_codesniffer/bin/phpcs",
        "lintfix": "./vendor/squizlabs/php_codesniffer/bin/phpcbf"
    },
    "extra": {
        "google/apiclient-services": [
            "Drive",
            "YouTube",
            "YouTubeAnalytics",
            "YouTubeReporting"
        ]
    }
}
